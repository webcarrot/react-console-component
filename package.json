{
  "author": {
    "email": "admin@webcarrot.pl",
    "name": "webcarrot"
  },
  "bugs": {
    "url": "https://github.com/webcarrot/react-console-component/issues"
  },
  "description": "Simple react component that display console messages",
  "main": "./distribution/index.js",
  "installable": false,
  "keywords": [
    "console",
    "react",
    "react-component"
  ],
  "scripts": {
    "lint": "eslint ./source",
    "format": "esformatter -i source/**/*.js",
    "build": "babel source --out-dir distribution",
    "prepublish": "npm run build"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/webcarrot/react-console-component/blob/master/LICENSE"
    }
  ],
  "maintainers": [
    {
      "name": "webcarrot",
      "email": "admin@webcarrot.pl"
    }
  ],
  "name": "react-console-component",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/webcarrot/react-console-component.git"
  },
  "version": "0.0.4",
  "esformatter": {
    "plugins": [
      "esformatter-quotes",
      "esformatter-jsx"
    ],
    "quotes": {
      "type": "double",
      "avoidEscape": false
    },
    "jsx": {
      "formatJSX": true,
      "attrsOnSameLineAsTag": false,
      "maxAttrsOnTag": 1,
      "firstAttributeOnSameLine": true,
      "alignWithFirstAttribute": true,
      "spaceInJSXExpressionContainers": "",
      "htmlOptions": {}
    }
  },
  "jshintConfig": {
    "esnext": true,
    "node": true,
    "strict": true,
    "unused": true
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended"
    ],
    "rules": {
      "no-underscore-dangle": 0,
      "quotes": 0,
      "no-unused-vars": 1,
      "no-console": 0
    },
    "plugins": [
      "react"
    ],
    "ecmaFeatures": {
      "arrowFunctions": true,
      "blockBindings": true,
      "jsx": true,
      "modules": true,
      "forOf": true
    },
    "env": {
      "browser": true,
      "node": true,
      "es6": true
    }
  },
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-eslint": "^6.0.5",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "esformatter": "^0.9.5",
    "esformatter-jsx": "^7.0.0",
    "esformatter-quotes": "^1.1.0",
    "eslint": "^2.13.1",
    "eslint-plugin-react": "^5.2.2"
  },
  "peerDependencies": {
    "react": "^0.14.0 || ^15.0.0-rc.1"
  }
}
